<ul class="list-group">
    {% if name is defined %}
    <li class="list-group-item">
        {{ 'process_log.revision.table.log_entry.process_client_person.name'|trans }}: {{ name ?: '-' }}
    </li>
    {% endif %}

    {% if address is defined %}
    <li class="list-group-item">
        {{ 'process_log.revision.table.log_entry.process_client_person.address'|trans }}: {{ address ?: '-' }}
    </li>
    {% endif %}

    {% if postalCode is defined %}
    <li class="list-group-item">
        {{ 'process_log.revision.table.log_entry.process_client_person.postal_code'|trans }}: {{ postalCode ?: '-' }}
    </li>
    {% endif %}

    {% if city is defined %}
    <li class="list-group-item">
        {{ 'process_log.revision.table.log_entry.process_client_person.city'|trans }}: {{ city ?: '-' }}
    </li>
    {% endif %}

    {% if telephone is defined %}
    <li class="list-group-item">
        {{ 'process_log.revision.table.log_entry.process_client_person.telephone'|trans }}: {{ telephone ?: '-' }}
    </li>
    {% endif %}

    {% if numberOfChildren is defined %}
    <li class="list-group-item">
        {{ 'process_log.revision.table.log_entry.process_client_person.number_of_children'|trans }}: {{ numberOfChildren ?: '-' }}
    </li>
    {% endif %}

    {% if carRegistrationNumber is defined %}
    <li class="list-group-item">
        {{ 'process_log.revision.table.log_entry.process_client_person.car_registration_number'|trans }}: {{ carRegistrationNumber ?: '-' }}
    </li>
    {% endif %}

    {% if receivesPublicAid is defined %}
    <li class="list-group-item">
        {{ 'process_log.revision.table.log_entry.process_client_person.receives_public_aid'|trans }}: {{ receivesPublicAid|yes_no }}
    </li>
    {% endif %}

    {% if employed is defined %}
        <li class="list-group-item">
            {{ 'process_log.revision.table.log_entry.process_client_person.employed'|trans }}: {{ employed|yes_no }}
        </li>
    {% endif %}

    {% if hasOwnCompany is defined %}
        <li class="list-group-item">
            {{ 'process_log.revision.table.log_entry.process_client_person.has_own_company'|trans }}: {{ hasOwnCompany|yes_no }}
        </li>
    {% endif %}
</ul>
